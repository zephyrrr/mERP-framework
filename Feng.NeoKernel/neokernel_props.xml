		<session_timeout_ms>30000</session_timeout_ms>
		<session_type>session</session_type>
		<startup>true</startup>
	</props>
	
	<props>
		<agent_id>151</agent_id>
		<allow_directory_lists>yes</allow_directory_lists>
		<classname>com.neokernel.httpd.HTTPFileServerAgent</classname>
		<hide_debug>false</hide_debug>
		<hide_println>false</hide_println>
		<hide_warning>false</hide_warning>
		<html_directory>./Web</html_directory>
		<name>HTTPFileServerAgent</name>
		<startup>true</startup>
	</props>
	
	<props>
		<classname>com.neokernel.httpd.HTTPPutAgent</classname>
		<html_directory>./Web/</html_directory>
		<make_directories>true</make_directories>
		<service_name>put_agent</service_name>
		<startup>true</startup>
	</props>
	
	<props>
		<agent_id>175</agent_id>
		<bind_port>8088</bind_port>
		<classname>com.neokernel.httpd.WebServer</classname>
		<hide_debug>false</hide_debug>
		<hide_println>false</hide_println>
		<hide_warning>false</hide_warning>
		<index_agent>HTTPFileServerAgent</index_agent>
		<log_file_dir>.\logs\</log_file_dir>
		<log_requests>true</log_requests>
		<max_connections>20</max_connections>
		<multihome>true</multihome>
		<name>WebServer</name>
		<socket_linger_time>2000</socket_linger_time>
		<startup>true</startup>
		<http_put.agent>put_agent</http_put.agent>
		<thread_pool_timeout>20000</thread_pool_timeout>
	</props>
	
	<props>
		<agent_id>120</agent_id>
		<classname>com.neokernel.om.AgentServer</classname>
		<name>AgentServer</name>
		<om_service_name>object_manager</om_service_name>
