		<service_name>object_manager</service_name>
		<startup>true</startup>
	</props>
	<props>
		<agent_id>125</agent_id>
		<classname>com.neokernel.om.SessionManager</classname>
		<name>SessionManager</name>
		<om_service_name>object_manager</om_service_name>
		<service_name>session_manager</service_name>
		<session_type>session</session_type>
		<startup>true</startup>
	</props>
	<props>
