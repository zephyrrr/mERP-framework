<?xml version="1.0"?>
<props_list>
<props>
		<agent_id>100</agent_id>
		<classname>com.neokernel.nk.ServiceManager</classname>
		<hide_debug>true</hide_debug>
		<name>ServiceManager</name>
		<startup>true</startup>
	</props>
	<props>
		<agent_id>115</agent_id>
		<classname>com.neokernel.om.FileObjectManager</classname>
		<file_store_dir>./</file_store_dir>
		<name>FileObjectManager</name>
